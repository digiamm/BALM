<launch>
	<arg name="rviz" default="true" />

	<param name="voxel_size" type="double" value="1" />
	<!-- <param name="file_path" type="string" value="/tmp/quadeasy_pcl/quadeasy_trajectory.txt" /> -->
	<param name="file_path" type="string" value="/tmp/cloister_pcl/cloister_trajectory.txt" />

	<param name="trajectory_output_path" type="string" value="/tmp/cloister_pcl/balm_trajectory.txt" />
	<node pkg="balm2" type="benchmark_realworld" name="benchmark_realworld" output="screen" />

	<group if="$(arg rviz)">
		<node launch-prefix="nice" pkg="rviz" type="rviz" name="rviz" args="-d $(find balm2)/rviz_cfg/back.rviz" />
	</group>  

</launch>
